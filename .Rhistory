hours <- c(as.character(CH4_re02$hour))
dateL <- c(as.character(CH4_re02$variable))
time_24hr_H <- c(paste(dateL, hours, sep = " "))
time_24hr <- c(paste(time_24hr_H, ":00:00", sep = ""))
