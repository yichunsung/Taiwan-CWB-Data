DL_CH4_plotly_00 <- plot_ly(data = DL_CH4, x=DL_CH4$date, y=DL_CH4$`00`,  type = "scatter", mode = "liners") # 先看看0時狀況
# 重新整理Data
# Reload Data Frame
CH4_re_no <- c(0:23)
